{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=MyAuthApp.db"
  },
  "JwtSettings": {
    "Key": "Your_256bit_Long_Secret_Key_Here",
    "Issuer": "MyAuthApp",
    "Audience": "MyAuthAppUsers",
    "ExpiryMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
